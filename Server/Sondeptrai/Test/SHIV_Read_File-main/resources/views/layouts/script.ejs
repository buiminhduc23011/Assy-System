<script>
    var socket = io();
    let pathName = window.location.pathname.split('/').filter(e => String(e).trim());
    // Remove all class active
    $('.active').removeClass('active');
    // Add class active using url
    if(pathName.length == 0) {
        $('.dashboard').addClass('active');
    } else {
        $(`.${pathName[pathName.length - 1]}`).addClass('active');
    }
</script>